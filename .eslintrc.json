{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": ["error", 3],
        "no-console": "off",
        "no-underscore-dangle": "off",
        "linebreak-style": "off"
    }
}
